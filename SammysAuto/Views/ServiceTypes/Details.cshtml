@model SammysAuto.Models.ServiceType
@{
    ViewData["Title"] = "Details";
}

<br />
<h2>Detalhes do tipo de serviço</h2>
<hr />

<div class="form-border">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-1">
                <label asp-for="Nome" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input asp-for="Nome" disabled class="form-control" />
            </div>
        </div>
    </div>
    <br />
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-sm btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-sm btn-success">De volta à lista</a>
    </div>
</div>

